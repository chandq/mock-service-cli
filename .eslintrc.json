{
  "extends": "eslint-config-populist",
  "rules": {
    "strict": "warn",
    "indent": ["warn", 2],
    "valid-jsdoc": "off",
    "no-sync": "off",
    "quote-props": "off",
    "no-continue": "off",
    "no-console": "off",
    "guard-for-in": "warn",
    "no-undefined": "warn",
    "comma-dangle": "warn",
    "maxstatements": 150,
    "callback-return": ["warn", ["next"]]
  }
}
